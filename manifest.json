{
  "manifest_version": 2,

  "name": "BankID fix for Swedbank",
  "description": "This extension makes BankID the default authentication method for Swedbank",
  "version": "1.0",

  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },

  "content_scripts": [
    {
        "matches": [ "https://internetbank.swedbank.se/*" ],
        "js": ["content.js"],
        "run_at" : "document_idle"
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
